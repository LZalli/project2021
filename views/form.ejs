<style>
body {
    color: #404E67;
    background: #F5F7FA;
    font-family: 'Open Sans', sans-serif;
}

</style>

</head>
<body>
    <form action="/facebook" method="POST" name="nmyForm" id="myForm">


  <!--<div class="form-group">
    <label for="exampleInputpageid1">pageid</label>
    <input type="text" class="form-control" id="exampleInputpageid1" placeholder="pageid" name="pageid">
  </div> -->

  <div>
      <select class="form-control" name="pageid">
        <% mylist.forEach((pages)=> { %>

            <option value="<%= pages.pageid %>"><%= pages.pagename%></option> 

            <% }) %>
        </select>
  
    </div>  
  <div class="form-group">
    <label for="exampleInputpageid1">startdate</label>
    <input type="date" class="form-control" id="exampleInputpageid1" placeholder="start" name="startdate">
  </div>
  <div class="form-group">
    <label for="exampleInputpageid1">enddate</label>
    <input type="date" class="form-control" id="exampleInputpageid1" placeholder="end" name="enddate">
  </div>
 
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
  
</body>
